# ===========================================
# 全局环境变量模板
# 复制此文件为 .env 并填入实际值
# ===========================================

# 数据库配置
DB_HOST=host.docker.internal
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_secure_password
DB_NAME=your_database

# Redis 配置（如需要）
REDIS_HOST=host.docker.internal
REDIS_PORT=6379

# API 密钥
SECRET_KEY=your_complex_random_string_here
OPENAI_API_KEY=sk-xxxxxxxxxxxx

# 应用配置
DEBUG=false
LOG_LEVEL=INFO
